{
    "openapi": "3.0.0",
    "info": {
        "title": "Eoogle Analytics",
        "version": "1.0.0"
    },
    "paths": {
        "/analytics/hits//by-link/{link}": {
            "get": {
                "tags": [
                    "hits"
                ],
                "summary": "getHitsByLinkInInterval",
                "operationId": "getHitsByLinkInInterval",
                "parameters": [
                    {
                        "name": "link",
                        "in": "path",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "name": "start_date",
                        "in": "query",
                        "required": true,
                        "schema": {
                            "type": "date"
                        }
                    },
                    {
                        "name": "end_date",
                        "in": "query",
                        "required": true,
                        "schema": {
                            "type": "date"
                        }
                    }
                ],
                "requestBody": {
                    "description": "Pass user credentials",
                    "required": true
                },
                "responses": {
                    "422": {
                        "description": "Wrong credentials response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "example": "Sorry, wrong email address or password. Please try again"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/analytics/hits//by-type/{linkType}": {
            "get": {
                "tags": [
                    "hits"
                ],
                "summary": "getHitsByLinkInInterval",
                "operationId": "getHitsByLinkInInterval",
                "parameters": [
                    {
                        "name": "linkType",
                        "in": "path",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "name": "start_date",
                        "in": "query",
                        "required": true,
                        "schema": {
                            "type": "string",
                            "format": "date-time"
                        }
                    },
                    {
                        "name": "end_date",
                        "in": "query",
                        "required": true,
                        "schema": {
                            "type": "string",
                            "format": "date-time"
                        }
                    }
                ],
                "requestBody": {
                    "description": "Pass user credentials",
                    "required": true
                },
                "responses": {
                    "422": {
                        "description": "Wrong credentials response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "example": "Sorry, wrong email address or password. Please try again"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/user-journey/{customer_uuid}": {
            "get": {
                "tags": [
                    "user-journey"
                ],
                "summary": "getUserJourney",
                "operationId": "getUserJourney",
                "parameters": [
                    {
                        "name": "customer_uuid",
                        "in": "path",
                        "required": true,
                        "schema": {
                            "type": "uuid"
                        }
                    }
                ],
                "responses": {
                    "422": {
                        "description": "Wrong credentials response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "example": "Sorry, wrong email address or password. Please try again"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/user-journey/{customer_uuid}/similar": {
            "get": {
                "tags": [
                    "user-journey"
                ],
                "summary": "getUsersJourneyByUserIdentifier",
                "operationId": "getUsersJourneyByUserIdentifier",
                "parameters": [
                    {
                        "name": "customer_uuid",
                        "in": "path",
                        "required": true,
                        "schema": {
                            "type": "uuid"
                        }
                    }
                ],
                "responses": {
                    "422": {
                        "description": "Wrong credentials response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "example": "Sorry, wrong email address or password. Please try again"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/statistics": {
            "post": {
                "tags": [
                    "Statistics"
                ],
                "operationId": "store",
                "requestBody": {
                    "description": "Pet object that needs to be added to the store",
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/StoreStatisticsRequest"
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "Successful operation"
                    },
                    "400": {
                        "description": "Bad Request"
                    }
                }
            }
        }
    },
    "components": {
        "schemas": {
            "StoreStatisticsRequest": {
                "required": [
                    "link",
                    "link_type",
                    "customer_uuid"
                ],
                "properties": {
                    "link": {
                        "type": "integer",
                        "readOnly": "true",
                        "example": "1"
                    },
                    "link_type": {
                        "description": "User role",
                        "type": "string",
                        "readOnly": "true"
                    },
                    "customer_uuid": {
                        "description": "User unique email address",
                        "type": "string",
                        "format": "email",
                        "readOnly": "true",
                        "example": "user@gmail.com"
                    }
                },
                "type": "object",
                "xml": {
                    "name": "StoreStatisticsRequest"
                }
            },
            "Statistics": {
                "title": "Statistics",
                "properties": {
                    "link": {
                        "title": "link",
                        "type": "string"
                    },
                    "link_type": {
                        "title": "link_type",
                        "type": "string"
                    },
                    "customer_uuid": {
                        "title": "customer_uuid",
                        "type": "string"
                    }
                },
                "type": "object"
            }
        }
    },
    "security": [
        []
    ]
